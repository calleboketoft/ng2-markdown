      "pre-commit": "npm run build"
    }
  },
  "keywords": ["angular2"],
  "author": "Carl-Ola Boketoft <calleboketoft@gmail.com>",
  "license": "ISC",
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.6",
    "highlight.js": "^9.6.0",
