  },
  "devDependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "bootstrap": "4.0.0-alpha.3",
    "commonmark": "0.26.0",
    "es6-shim": "0.35.1",
    "express": "4.14.0",
    "github-markdown-css": "2.4.1",
    "highlight.js": "9.7.0",
    "reflect-metadata": "0.1.8",
